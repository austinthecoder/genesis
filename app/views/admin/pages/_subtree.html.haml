- unless arranged_pages.empty?
  %ul
    - arranged_pages.each do |page, arranged_pages|
      %li{:id => "page_#{page.id}"}
        = link_to page.title, edit_admin_page_path(page)
        |
        = link_to "add subpage", new_admin_page_page_path(page)
        = render "subtree", :arranged_pages => arranged_pages